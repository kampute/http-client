{
  "metadata": [
    {
      "src": [
        {
          "files": [ "src/**/*.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ],
          "properties": {
            "TargetFrameworks": "netstandard2.0;netstandard2.1",
            "OutputPaths": [
              "bin/Release/netstandard2.0",
              "bin/Release/netstandard2.1"
            ]
          },
          "codeProjectProperties": {
            "ProjectReferences": [
              {
                "ProjectPath": "**/*.csproj"
              }
            ]
          }
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": "**/*.{md,yml}"
      },
      {
        "files": "README.md"
      },
      {
        "files": "ICON.png"
      },
      {
        "files": "LICENSE"
      }
    ],
    "globalMetadata": {
      "_appTitle": "Kampute.HttpClient",
      "_appFooter": "Â© 2024 Kampute",
      "_appFaviconPath": "ICON.png",
      "_appLogoPath": "ICON.png"
    },
    "template": [ "default" ],
    "dest": "docs"
  }
}
